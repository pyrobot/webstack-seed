include mixins/loader
- var pageLang = pageLang || 'en'
- var debug = debug || false
- var startModule = startModule || 'myApp'
- var loadModule = debug ? startModule : null
!!!
html(lang=pageLang,ng-app=startModule)
  head
    meta(http-equiv='X-UA-Compatible',content='IE=8')
    meta(charset="utf-8")
    block metas
    +script_loader(debug)
    block styles
  body(ng-cloak)
    block content